{
   "author": "Alexander Shutov",
   "background": {
      "page": "background/background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "19": "img/dr_active_19.png",
         "38": "img/dr_active_38.png"
      },
      "default_popup": "popup/popup.html",
      "default_title": "Dark Reader"
   },
   "commands": {
      "addSite": {
         "description": "Toggle current site",
         "suggested_key": {
            "default": "Alt+Shift+A"
         }
      },
      "toggle": {
         "description": "Toggle On/Off",
         "suggested_key": {
            "default": "Alt+Shift+D"
         }
      }
   },
   "description": "Care your eyes, invert page colors, use dark theme for night and daily browsing.",
   "icons": {
      "128": "img/dr_128.png",
      "16": "img/dr_16.png",
      "48": "img/dr_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Dark Reader",
   "permissions": [ "fontSettings", "storage", "tabs", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.5.0"
}
