{
    "OBSOLETE": "use fix_inversion.json",
    "commonSelectors": [
        "img, iframe, video, *:not(object):not(body)>embed, object",
        "svg image",
        "*[style*=\"background:url\"]:empty, *[style*=\"background-image:url\"]:empty",
        "*[style*=\"background: url\"]:empty, *[style*=\"background-image: url\"]:empty",
        "twitterwidget"
    ],
    "specials": [
        {
            "url": "airbnb.com",
            "selectors": [
                "{common}",
                ".background-cover, #footer, #photo-modal",
                "*[style*=\"background-image:url\"], *[style*=\"background-image: url\"]"
            ],
            "rules": "#footer *, #photo-modal * { -webkit-filter: none !important; }"
        },
        {
            "url": "basho.com",
            "selectors": [
                "{common}",
                "#site-navigation, footer, header, .featured-news-wrapper",
                "*:not(.lang):not(#foot-logo)>img"
            ]
        },
        {
            "url": "bbc.com",
            "selectors": [
                "{common}",
                "div.orb-nav-section.orb-nav-blocks > a > img"
            ]
        },
        {
            "url": "facebook.com",
            "selectors": [
                "{common}",
                "._3ixn, ._2teu, .uiStreamStory",
                ".userContentWrapper canvas, ._4lqu, ._4lqt",
                "._24ws, ._1z0-"
            ],
            "rules": [
                "._4d3w .stageWrapper { background: white; }",
                ".uiStreamStory video { -webkit-filter: none !important; }"
            ]
        },
        {
            "url": "gigaom.com",
            "selectors": "{common}, .bg"
        },
        {
            "url": [
                "google.*",
                "google.*.*"
            ],
            "selectors": [
                "img:not(.irc_mi):not(.irc_rii):not(.irc_mut):not(.amI):not(.amJ):not(.adk):not(.adj):not([src*=\"ic_\"]):not([src*=\"black\"])",
                "iframe, video, embed, .gbii, .gbip, .azp, .aLF-aPX-KP, .irc_bg, .RY3tic, canvas.circle"
            ],
            "rules": [
                ".pI { background: rgba(255,255,255,.7) !important; }",
                ".eB { backgroud: rgba(255,255,255,.4) !important; }"
            ]
        },
        {
            "url": "granttree.co.uk",
            "selectors": "img, iframe, video, embed, .photo"
        },
        {
            "url": "ign.com",
            "selectors": "{common}, .video_embed_content-poster, .video_embed_content-poster-play-button",
            "rules": "#review-promo { background-color: white; }"
        },
        {
            "url": "inpad.com.tw",
            "selectors": "{common}, *[style*=\"background-image:url\"]"
        },
        {
            "url": "io9.com",
            "selectors": "{common}",
            "rules": "html { height: auto !important; }"
        },
        {
            "url": "lifehacker.com",
            "selectors": "{common}, .videoCube a .thumbBlock",
            "rules": "html { height: auto !important; }"
        },
        {
            "url": "linkedin.com",
            "selectors": [
                "{common}",
                "*[style*=\"background-image: url\"]",
                "#top-header, #responsive-nav-scrollable"
            ],
            "rules": [
                "#top-header *, #responsive-nav-scrollable * { -webkit-filter: none !important; }"
            ]
        },
        {
            "url": "mail.live.com",
            "selectors": [
                "img:not([src=\"https://a.gfx.ms/rte_metro2.png\"])",
                "video, object, .ComposeContent"
            ],
            "rules": ".ComposeContent { background-color: white; }"
        },
        {
            "url": "messenger.com",
            "selectors": "._4tsk"
        },
        {
            "url": [
                "music.yandex.by",
                "music.yandex.ru",
                "music.yandex.kz",
                "music.yandex.ua"
            ],
            "selectors":"a[style*=\"background-image: url\"]"
        },
        {
            "url": "onliner.by",
            "selectors": "{common}, .b-tile-main, .b-teasers-2__teaser, .news-tiles__subtitle, .b-opinions-main-2__tile, .news-header__top > .news-header__title",
            "rules": ".b-opinions-main-2__tile .b-opinions-main-2__portrait { -webkit-filter: none !important; }"
        },
        {
            "url": "pandora.com",
            "selectors": "#body, .contentbox, #detailContainer, #mainContent, .slidesBackground, .slidesForeground",
            "rules": [
                ".skinContainer, .slidesBackground, .slidesForeground, #trackDetail, .track_detail_close { background-color: black !important; }",
                ".fxCol-cont-1, .jspContainer, #trackInfoButtons { background-color: white !important; }",
                "#brandingBar, .contentnav, .top { background-color: #3d4043 !important;}",
                "#playerBar { background-color: #0d0d0d !important;}"
            ]
        },
        {
            "url": "play.afreecatv.com",
            "selectors": "{common}, #afreecatv_player"
        },
        {
            "url": "producthunt.com",
            "selectors": "{common}, .backgroundImage_1hK9M, .v-image",
            "rules": [
                ".backgroundImage_1hK9M img { -webkit-filter: none !important; }",
                ".backgroundImage_1hK9M .backgroundImage_1hK9M { -webkit-filter: none !important; }",
                ".v-image .post-detail--body--gallery-fullscreen--image { -webkit-filter: none !important; }",
                ".post-detail--body--gallery-fullscreen--image { background-color: black; }"
            ]
        },
        {
            "url": "reddit.com",
            "selectors": "{common}, #header-img"
        },
        {
            "url": "shop.ubi.com",
            "selectors": "{common}, #topNavbar, #UplayHeader"
        },
        {
            "url": "slack.com",
            "selectors": [
                "{common}",
                ".client_channels_list_container",
                "figure, *[style*=\"url\"]"
            ],
            "rules": [
                ".client_channels_list_container * { -webkit-filter: none !important; }",
                "figure *, *[style*=\"url\"] * { -webkit-filter: none !important; }"
            ]
        },
        {
            "url": "spotify.com",
            "selectors": [
                "img, iframe, video, embed",
                ".hero, .header-main, .search-bar, #bg-wrap, #main"
            ]
        },
        {
            "url": [
                "stackexchange.com",
                "askubuntu.com",
                "mathoverflow.com",
                "serverfault.com",
                "stackapps.com",
                "stackoverflow.com",
                "superuser.com"
            ],
            "selectors": "{common}",
            "rules": "#hlogo a { text-indent: -256em !important; }"
        },
        {
            "url": "tpondemand.com",
            "selectors": [
                "{common}",
                ".tau-cover-view__overlay, .tau-app-secondary-pane, .app-header, .uv-popover-content"
            ],
            "rules": [
                ".tau-app-secondary-pane *, .app-header * { -webkit-filter: none !important; }"
            ]
        },
        {
            "url": "twitch.tv",
            "selectors": [
                "{common}",
                "#site_footer, #carousel_background:after, #left_col, .player, .badge"
            ],
            "rules": [
                "#carousel, .chat-container, .conversations-list-header { background-color: white !important; }",
                ".dark_wrapper ul, html, .nav { background-color: white !important; }",
                ".js-conversations-list, .js-conversations-list-bottom-bar { background-color: white !important; }",
                ".rightcol-content > .top { background-color: white !important; }",
                ".chat_text_input, .colon, .dark_wrapper *, .message, .room-title, .selected > a { color:black !important;}",
                ".chat_text_input { border: 1px solid black !important;}",
                ".player * { -webkit-filter: none !important;}"
            ]
        },
        {
            "url": "twitter.com",
            "selectors": [
                "{common}",
                ".tcu-textEllipse--multiline"
            ]
        },
        {
            "url": "ubi.com",
            "selectors": "{common}",
            "rules": "html, body { background: white !important; }"
        },
        {
            "url": "vk.com",
            "selectors": [
                "{common}",
                "#video_player, *[style*=\"url\"], .mv_playlist, .page_header_cont"
            ],
            "rules": [
                "#video_player *, *[style*=\"url\"] *, .mv_playlist *, .page_header_cont * { -webkit-filter: none !important; }",
                ".mv_layer_bg { background: white !important; }"
            ]
        },
        {
            "url": "web.whatsapp.com",
            "selectors": "{common}, .qrcode .icon-logo",
            "rules": ".qrcode { border: 8px solid black; }"
        },
        {
            "url": "wikipedia.org",
            "selectors": "img:not(.mwe-math-fallback-image-inline), iframe, video, embed"
        },
        {
            "url": "wolfram.com",
            "selectors": "{common}",
            "rules": [
                ".numberedequation, .displayformula { -webkit-filter: none !important; }"
            ]
        },
        {
            "url": "youtube.com",
            "selectors": [
                "iframe, .html5-video-player, .has-custom-banner, #theater-background",
                "#watch-appbar-playlist",
                "*:not(.html5-video-player):not(.channel-header-profile-image-container):not(.html5-storyboard-filmstrip):not(.html5-storyboard-framepreview):not(.html5-storyboard-lens) > img"
            ],
            "rules": [
                "#watch-appbar-playlist * { -webkit-filter: none !important; }"
            ]
        }
    ]
}
